{
  "extends": "tslint:recommended",
  "rules": {
    "triple-equals": false,
     "switch-default": false,
     "quotemark": [
         "single"
     ],
     "max-line-length": [
     ],
     "member-access": [
       false
     ],
     "trailing-comma": [false],
     "object-literal-sort-keys": false,
     "eofline": false,
     "radix": false,
     "no-var-requires": false,
     "ordered-imports": [
       false
     ],
     "no-console": [
       false
     ],
     "arrow-parens": false,
     "only-arrow-functions": [
       false
     ],
     "variable-name": false,
     "no-debugger": false,
     "no-string-literal": false
  },
  "rulesDirectory": []
}